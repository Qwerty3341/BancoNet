<Window x:Class="Banco.UI.Main.Executive.DeleteAccount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Banco.UI.Main.Executive"
        mc:Ignorable="d"
        Title="Baja de cuenta" Height="250" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>


        </Grid.RowDefinitions>
        <Label Content="Ingresa el numero de cuenta" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="20"/>
        <TextBox HorizontalAlignment="Center" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Center" Width="150" Height="25"/>
        <Border Grid.Row="2">
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Button x:Name="Aceptar" Content="Aceptar" FontSize="16" Margin="10" Width="100" Height="50" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <Button x:Name="Cancelar" Content="Cancelar" FontSize="16" Margin="10" Width="100" Height="50" VerticalAlignment="Center" HorizontalAlignment="Center"/>
            </StackPanel>
        </Border>

    </Grid>
</Window>
