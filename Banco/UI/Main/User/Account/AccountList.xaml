             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Banco.UI.Main"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ScrollViewer x:Name="AccountScrollablePanel">
            <StackPanel x:Name="AccountStackPanel" Orientation="Vertical" Margin="0,0,0,0">
            </StackPanel>
        </ScrollViewer>
        <!--AKI VAN LOS PANELES DE LAS CUENTAS-->
    </Grid>
</UserControl>
